{
    "name": "redfieldchristabel/filament-blackbox",
    "description": "A professional-grade auditing UI for Filament PHP. Transform raw laravel-auditing data into a powerful, centralized 'Blackbox' flight recorder with beautiful diffs, relationship-aware timelines, and advanced filtering.",
    "keywords": [
        "redfieldchristabel",
        "laravel",
        "filamentphp",
        "filament-blackbox"
    ],
    "homepage": "https://github.com/redfieldchristabel/filament-blackbox",
    "support": {
        "issues": "https://github.com/redfieldchristabel/filament-blackbox/issues",
        "source": "https://github.com/redfieldchristabel/filament-blackbox"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "redfieldchristabel",
            "email": "redfieldchristabel@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^5.0",
        "owen-it/laravel-auditing": "^14.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^4.3",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-livewire": "^4.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "rector/rector": "^2.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Blackbox\\FilamentBlackbox\\": "src/",
            "Blackbox\\FilamentBlackbox\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blackbox\\FilamentBlackbox\\Tests\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "phpstan analyse",
        "lint": "pint",
        "refactor": "rector",
        "test": "pest",
        "test:lint": "pint --test",
        "test:refactor": "rector --dry-run"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Blackbox\\FilamentBlackbox\\FilamentBlackboxServiceProvider"
            ],
            "aliases": {
                "FilamentBlackbox": "Blackbox\\FilamentBlackbox\\Facades\\FilamentBlackbox"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
